{
  "name": "econ-graph-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "5.18.0",
    "@mui/material": "5.18.0",
    "@mui/x-date-pickers": "6.20.2",
    "@types/d3": "7.4.3",
    "@types/lodash": "4.17.20",
    "@types/node": "20.19.13",
    "@types/react": "18.3.24",
    "@types/react-dom": "18.3.7",
    "@types/topojson-client": "3.1.5",
    "ajv": "8.17.1",
    "axios": "^1.12.0",
    "chart.js": "4.5.0",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.1.0",
    "d3": "7.9.0",
    "date-fns": "2.30.0",
    "lodash": "4.17.21",
    "react": "18.3.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.3.1",
    "react-query": "3.39.3",
    "react-router-dom": "6.30.1",
    "react-scripts": "^5.0.1",
    "rxjs": "7.8.2",
    "topojson-client": "3.1.0",
    "web-vitals": "3.5.2"
  },
  "devDependencies": {
    "@apollo/client": "4.0.4",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "jest": "27.5.1",
    "jest-environment-jsdom": "29.7.0",
    "msw": "2.11.1",
    "prettier": "3.6.2",
    "react-test-renderer": "18.3.1",
    "resize-observer-polyfill": "1.5.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prettier-check": "prettier --check 'src/**/*.{js,ts,tsx,css}'",
    "prettier-fix": "prettier --write 'src/**/*.{js,ts,tsx,css}'",
    "typecheck": "tsc --noEmit",
    "eslint-check": "eslint src --ext .ts,.tsx",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jest/no-conditional-expect": "off",
      "testing-library/no-node-access": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.test.ts",
          "**/*.test.tsx",
          "**/__tests__/**/*"
        ],
        "rules": {
          "jest/no-conditional-expect": "off",
          "testing-library/no-node-access": "off"
        }
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^react-app/jest$": "react-app/jest",
      "^d3$": "<rootDir>/src/__mocks__/d3.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/d3-dispatch": "3.0.4"
  },
  "resolutions": {
    "@types/d3-dispatch": "3.0.4"
  },
  "proxy": "http://localhost:8080"
}
